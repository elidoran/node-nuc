{
  "name": "nuc",
  "version": "0.2.2",
  "description": "Configuration via JSON and INI from hierarchy of files.",
  "main": "lib",
  "bin": "bin/nuc.js",
  "author": "Eli Doran <eli+npm@elidoran.com>",
  "license": "MIT",
  "scripts": {
